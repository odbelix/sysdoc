{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proccessrule</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proccessRule.id }}</td>
            </tr>
            <tr>
                <th>Proccess_id</th>
                <td>{{ proccessRule.proccessid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ proccessRule.name }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ proccessRule.text }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{% if proccessRule.createdat %}{{ proccessRule.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proccessrule_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('proccessrule_edit', { 'id': proccessRule.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
