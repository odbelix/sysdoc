{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proccesssystem</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proccessSystem.id }}</td>
            </tr>
            <tr>
                <th>Proccess_id</th>
                <td>{{ proccessSystem.proccessid }}</td>
            </tr>
            <tr>
                <th>System_id</th>
                <td>{{ proccessSystem.systemid }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{% if proccessSystem.createdat %}{{ proccessSystem.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{% if proccessSystem.updatedat %}{{ proccessSystem.updatedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proccesssystem_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('proccesssystem_edit', { 'id': proccessSystem.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
